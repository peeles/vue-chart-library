    "build-storybook": "storybook build",
    "lint": "eslint . --ext .js,.mjs,.vue --fix"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@storybook/addon-links": "^10.0.2",
