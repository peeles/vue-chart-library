    "@storybook/vue3-vite": "^10.0.2",
    "@tailwindcss/vite": "^4.1.16",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.0",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "postcss": "^8.4.49",
    "storybook": "^10.0.2",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.12",
