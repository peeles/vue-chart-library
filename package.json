  "description": "A customizable chart library for Vue 3",
  "type": "module",
  "main": "./dist/vue-charts.umd.cjs",
  "module": "./dist/vue-charts.js",
  "exports": {
    ".": {
      "import": "./dist/vue-charts.js",
      "require": "./dist/vue-charts.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
